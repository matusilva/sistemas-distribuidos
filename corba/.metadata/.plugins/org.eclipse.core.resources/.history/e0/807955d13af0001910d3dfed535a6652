package OlaApp;
import org.omg.CORBA.ORB;
import OlaApp.OlaPOA;

public class OlaServo extends OlaPOA {
	
	private String msg = "\n Olá App\n";
	private ORB orb;
	
	public void setORB(ORB orb) {
		this.orb = orb;
	}
	
	@Override
	public String msg() {
		// TODO Auto-generated method stub
		return msg;
	}

	@Override
	public void msg(String newMsg) {
		// TODO Auto-generated method stub
		msg = newMsg;
		
	}

}
